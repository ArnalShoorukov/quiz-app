<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="scroll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">4dp</item>

    </style>

    <style name="linear1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>

    </style>

    <style name="linear2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginBottom">16dp</item>
    </style>

    <style name="linear3">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="linear4">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="name">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:hint">Name</item>
        <item name="android:inputType">textCapWords</item>
    </style>

    <style name="intro">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:text">@string/intro</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">#212121</item>

    </style>

    <style name="seperator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">@android:color/darker_gray</item>
    </style>

    <style name="firstText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:text">@string/first</item>
        <item name="android:textColor">#191919</item>
    </style>
    <style name="firstEdit">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:hint">answer</item>
    </style>
    <style name="secondText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:text">@string/second</item>
        <item name="android:textColor">#191919</item>
    </style>

    <style name="checkA">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/a</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>
    <style name="checkB">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/b</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>
    <style name="checkC">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/c</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>
    <style name="checkD">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/d</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>

    <style name="thirdText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:text">@string/third</item>
        <item name="android:textColor">#191919</item>
    </style>

    <style name="image">
        <item name="android:layout_width">1300dp</item>
        <item name="android:layout_height">150dp</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:src">@drawable/cone</item>
    </style>

    <style name="thirdAnswer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="android:hint">answer</item>
    </style>

    <style name="fourthText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:text">@string/four</item>
        <item name="android:textColor">#191919</item>
    </style>

    <style name="fourthAnswer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:hint">answer</item>
    </style>

    <style name="fifthText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:text">@string/fifth</item>
        <item name="android:textColor">#191919</item>
    </style>

    <style name="sixthText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:text">@string/sixth</item>
        <item name="android:textColor">#191919</item>
    </style>
    <style name="checkA5">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:text">@string/a5</item>
        <item name="android:textSize">12sp</item>
        <item name="android:padding">2dp</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>

    <style name="checkB5">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:text">@string/b5</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:padding">2dp</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>

    <style name="checkC5">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:text">@string/c5</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:padding">2dp</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>

    <style name="checkD5">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:text">@string/d5</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:padding">2dp</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>

    <style name="submit">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:onClick">submit</item>
        <item name="android:padding">20dp</item>
        <item name="android:text">@string/submit</item>
        <item name="android:layout_marginLeft">24dp</item>
        <item name="android:layout_marginRight">24dp</item>
        <item name="android:background">@android:color/holo_blue_light</item>
    </style>

    <style name="share">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:onClick">share</item>
        <item name="android:padding">20dp</item>
        <item name="android:text">@string/share</item>
        <item name="android:layout_marginLeft">24dp</item>
        <item name="android:layout_marginRight">24dp</item>
        <item name="android:background">@android:color/holo_purple</item>
    </style>

    <style name="rGroup">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="r1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">1500</item>
    </style>

    <style name="r2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">1000</item>
    </style>

</resources>
